{
  "name": "flowershop-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^16.4.1",
    "react-loader": "^2.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "sass-build": "node-sass-chokidar src/styles/sass/index.sass -o src/styles/css/ --indented-syntax",
    "sass-watch": "node-sass-chokidar src/styles/sass/ -o src/styles/css/ --indented-syntax --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p sass-watch start-js",
    "build": "npm-run-all -p sass-build build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "git push heroku master"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "^2.13.5"
  }
}
